# BEGIN_KITTY_THEME
# Catppuccin-Macchiato
include themes/catppuccin_macchiato.conf
# END_KITTY_THEME

let &t_SI = "\<Esc>[6 q"
let &t_SR = "\<Esc>[4 q"
let &t_EI = "\<Esc>[2 q"


# BEGIN_KITTY_FONTS
font_family      family="DejaVuSansM Nerd Font Mono"
bold_font        auto
italic_font      auto
bold_italic_font auto

font_size 10.0
# END_KITTY_FONTS

cursor #ccFFcc
cursor_beam_thickness 2

inactive_text_alpha 0.5
